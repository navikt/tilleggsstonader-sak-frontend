{
  "name": "tilleggsstonader-sak-frontend",
  "version": "1.0.0",
  "repository": "git@github.com:navikt/tilleggsstonader-sak-frontend.git",
  "license": "MIT",
  "private": true,
  "type": "module",
  "scripts": {
    "ci": "yarn --prefer-offline --frozen-lockfile && yarn --cwd src/backend ci",
    "build": "yarn lint && webpack --config ./webpack/webpack.production.js && yarn --cwd src/backend build",
    "build:dev": "cd src/backend && yarn build",
    "start:dev": "cd src/backend && yarn start:dev",
    "start:dev-preprod": "cd src/backend && yarn start:dev-preprod",
    "prepare": "husky",
    "lint": "eslint 'src/**/*' --max-warnings=0",
    "lint:fix": "eslint 'src/**/*' --fix"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css}": [
      "prettier --write",
      "eslint --fix --max-warnings=0 --no-warn-ignored"
    ]
  },
  "dependencies": {
    "@navikt/aksel-icons": "^7.28.1",
    "@navikt/ds-css": "^7.28.1",
    "@navikt/ds-react": "^7.28.1",
    "@navikt/ds-tokens": "^7.20.0",
    "@navikt/familie-endringslogg": "^14.0.0",
    "@navikt/familie-form-elements": "^21.0.4",
    "@portabletext/react": "^4.0.2",
    "@sanity/client": "^7.8.2",
    "@unleash/proxy-client-react": "^5.0.0",
    "constate": "^3.3.3",
    "date-fns": "^4.1.0",
    "groq": "^4.6.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-pdf": "^10.1.0",
    "react-router-dom": "^7.8.1",
    "react-router-prompt": "^0.8.0",
    "styled-components": "^6.1.19",
    "unleash-proxy-client": "^3.7.6",
    "use-debounce": "^10.0.5",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/compat": "^1.3.0",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.33.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/uuid": "^10.0.0",
    "@types/webpack-hot-middleware": "^2.25.6",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "css-loader": "^7.1.1",
    "esbuild-loader": "^4.3.0",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-n": "^17.21.3",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-unused-imports": "^4.1.3",
    "fork-ts-checker-webpack-plugin": "^9.1.0",
    "globals": "^16.3.0",
    "html-webpack-plugin": "^5.6.3",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "prettier": "3.6.2",
    "style-loader": "^4.0.0",
    "typescript": "^5.9.2",
    "webpack": "^5.101.2",
    "webpack-cli": "^6.0.1"
  },
  "resolutions": {
    "styled-components": "^6"
  }
}
