{
  "name": "tilleggsstonader-sak-frontend",
  "version": "1.0.0",
  "repository": "git@github.com:navikt/tilleggsstonader-sak-frontend.git",
  "license": "MIT",
  "private": true,
  "type": "module",
  "scripts": {
    "ci": "yarn --prefer-offline --frozen-lockfile && yarn --cwd src/backend ci",
    "build": "yarn lint && webpack --config ./webpack/webpack.production.js && yarn --cwd src/backend build",
    "build:dev": "cd src/backend && yarn build",
    "start:dev": "cd src/backend && yarn start:dev",
    "start:dev-preprod": "cd src/backend && yarn start:dev-preprod",
    "prepare": "husky",
    "lint": "eslint 'src/**/*' --max-warnings=0",
    "lint:fix": "eslint 'src/**/*' --fix"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css}": [
      "prettier --write",
      "eslint --fix --max-warnings=0 --no-warn-ignored"
    ]
  },
  "dependencies": {
    "@navikt/aksel-icons": "^6.7.1",
    "@navikt/ds-css": "^7.5.3",
    "@navikt/ds-react": "^7.5.3",
    "@navikt/ds-tokens": "^7.4.0",
    "@navikt/familie-endringslogg": "^13.1.4",
    "@navikt/familie-form-elements": "^15.0.0",
    "@portabletext/react": "^3.0.9",
    "@sanity/client": "^6.22.5",
    "@unleash/proxy-client-react": "^4.4.0",
    "constate": "^3.3.2",
    "data-fns": "^1.1.0",
    "groq": "^3.64.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-pdf": "^9.1.1",
    "react-router-dom": "^6.27.0",
    "react-router-prompt": "^0.7.0",
    "styled-components": "^6.1.0",
    "unleash-proxy-client": "^3.4.0",
    "use-debounce": "^10.0.4",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.7",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.23.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/uuid": "^10.0.0",
    "@types/webpack-hot-middleware": "^2.25.6",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "css-loader": "^7.1.1",
    "esbuild-loader": "^4.3.0",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-n": "^17.17.0",
    "eslint-plugin-prettier": "^5.2.5",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unused-imports": "^4.1.3",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "globals": "^16.0.0",
    "html-webpack-plugin": "^5.6.3",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.0",
    "prettier": "3.5.3",
    "style-loader": "^4.0.0",
    "typescript": "^5.8.2",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1"
  },
  "resolutions": {
    "styled-components": "^6"
  }
}
